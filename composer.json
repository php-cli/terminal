{
  "name": "cli/terminal",
  "description": "MC-Style PHP Console Application",
  "keywords": [],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Buchnev",
      "email": "butschster@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Butschster\\Commander\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=8.3",
    "symfony/console": "^7.3",
    "symfony/finder": "^7.3",
    "symfony/process": "^7.3",
    "composer/composer": "^2.0"
  },
  "require-dev": {
    "spiral/code-style": "^2.2",
    "phpunit/phpunit": "^10.5",
    "rector/rector": "^2.1",
    "vimeo/psalm": "^6.0",
    "buggregator/trap": "^1.13"
  },
  "scripts": {
    "cs-fix": "vendor/bin/php-cs-fixer fix",
    "psalm": "vendor/bin/psalm --config=psalm.xml ./src",
    "refactor": "rector process --config=rector.php",
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
